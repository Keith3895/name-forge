{
  "name": "name-forge",
  "version": "1.0.0",
  "description": "A random name generator.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "copy": "copyfiles ./package.json dist && copyfiles ./ReadMe.md dist",
    "build": "tsc && grunt && npm run copy",
    "tag": "git tag -a $npm_package_version -m $npm_package_version && git push origin $npm_package_version"
  },
  "keywords": [
    "name generator",
    "random name",
    "fullname generator",
    "name-generator",
    "random-names",
    "name-generation",
    "random-generator",
    "name-creator"
  ],
  "author": "Keith Franklin",
  "license": "MIT",
  "dependencies": {
    "copyfiles": "^2.4.1",
    "grunt": "^1.6.1",
    "grunt-contrib-uglify": "^5.2.2",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2"
  }
}
